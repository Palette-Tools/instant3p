{
  "name": "@instant3p/storybook",
  "version": "0.20.12",
  "description": "3rd party Storybook plugin for Instant with isolated offline database instances",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "default": "./dist/preset.js"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "default": "./dist/preview.js"
    },
    "./dist/preview.js": {
      "types": "./dist/preview.d.ts",
      "default": "./dist/preview.js"
    }
  },
  "peerDependencies": {
    "@storybook/components": "9.0.0-alpha.1",
    "@storybook/core-events": "9.0.0-alpha.1",
    "@storybook/manager-api": "9.0.0-alpha.1",
    "@storybook/preview-api": "9.0.0-alpha.1",
    "@storybook/types": "9.0.0-alpha.1",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "check": "tsc --noEmit",
    "demo": "cd demo && npm run dev",
    "publish-package": "pnpm publish --access public --no-git-checks"
  },
  "dependencies": {
    "@instant3p/core-offline": "workspace:*",
    "@instant3p/react-offline": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.8.3",
    "vitest": "^1.6.0"
  }
}